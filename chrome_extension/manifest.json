{
    "manifest_version": 3,
    "name": "TwittEx",
    "version": "0.0.0.1",
    "description": "A Chrome extension to exclude tweets from your Twitter feed based on a regular expression.",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "128": "icon128.png"
    }
}